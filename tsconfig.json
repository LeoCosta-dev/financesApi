{
  "compilerOptions": {
    "target": "es2022",
    "lib":["ES5","ES6","ES2018", "ES2022"],
    "module": "Node16",
    "rootDir": "./",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "ts-node": { "transpileOnly": true }
}