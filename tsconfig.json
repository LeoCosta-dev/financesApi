{
  "compilerOptions": {
    "target": "es2016",
    "lib":["ES5","ES6","ES2018"],
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "ts-node": { "transpileOnly": true }
}